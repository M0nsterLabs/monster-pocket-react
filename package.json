{
  "name": "plasma-pocket-react",
  "version": "1.2.6",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && webpack-dev-server",
    "build:local": "NODE_ENV=local webpack",
    "build:beta": "NODE_ENV=beta node_modules/.bin/webpack",
    "build:production": "NODE_ENV=production node_modules/.bin/webpack",
    "build:dev": "node_modules/.bin/webpack",
    "build:demo2": "NODE_ENV=demo2 node_modules/.bin/webpack",
    "build:demo3": "NODE_ENV=demo3 node_modules/.bin/webpack",
    "build:demo4": "NODE_ENV=demo4 node_modules/.bin/webpack",
    "build": "rm -rf ./build && webpack",
    "watch": "webpack -w",
    "rebuild": "npm install",
    "test:jest": "jest",
    "test": "./node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill --require bluebird --require isomorphic-fetch",
    "test:coverage": "./node_modules/.bin/istanbul cover --report clover ./node_modules/.bin/_mocha -- -u exports -R spec test/**/*  --compilers js:babel-register --require babel-polyfill --require bluebird --require isomorphic-fetch",
    "test-coverage": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "config.js": "<rootDir>/config/application.local.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Plasma-Platform/Plasma-Pocket-React.git"
  },
  "author": "stevenreed",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "classlist": "^1.0.1",
    "css-loader": "^0.26.1",
    "currency-formatter": "^1.1.1",
    "ejs": "^2.5.5",
    "ejs-html-loader": "^2.0.2",
    "ejs-loader": "^0.3.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.29.0",
    "i": "^0.3.5",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^1.0.0-alpha",
    "jed": "^1.1.1",
    "jest": "^18.0.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.2",
    "mocha": "^3.2.0",
    "nools": "^0.4.4",
    "npm": "^4.5.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "po2json": "^0.4.5",
    "postcss-inline-svg": "^2.3.0",
    "postcss-loader": "^1.2.0",
    "postcss-safe-parser": "^2.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-interpolate-component": "^0.10.0",
    "react-router": "^3.0.0",
    "shortid": "^2.2.6",
    "style-loader": "^0.13.1",
    "transfer-webpack-plugin": "^0.1.4",
    "url-loader": "^0.5.9",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^2.3.1",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "dependencies": {
    "plasma-membership-api-client-js": "^1.0.17",
    "plasma-order-api-client-js": "^1.0.7",
    "plasma-reviews-api-client-js": "^1.0.42",
    "qs": "^6.2.1",
    "quark": "git+https://git@github.com/templatemonster/QUARK-React.git#feature/PTR-2225",
    "tm-products-api-client-js": "^1.0.14"
  },
  "babel": {
    "presets": [
      "react",
      "latest",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ],
      [
        "resolver",
        {
          "resolveDirs": [
            "src"
          ]
        }
      ]
    ]
  }
}

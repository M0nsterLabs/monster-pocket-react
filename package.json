{
  "name": "plasma-pocket-react",
  "version": "1.1.14",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && webpack-dev-server",
    "build:production": "rm -rf ./build && NODE_ENV=production webpack",
    "build": "rm -rf ./build && webpack",
    "prepublish": "npm run build:production",
    "watch": "webpack -w",
    "rebuild": "npm install",
    "test:jest": "jest",
    "test": "mocha --compilers js:babel-register",
    "test-coverage": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Plasma-Platform/Plasma-Pocket-React.git"
  },
  "author": "stevenreed",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "classlist": "^1.0.1",
    "css-loader": "^0.26.1",
